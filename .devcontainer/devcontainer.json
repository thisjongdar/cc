  "name": "Codespace Command Executor",
  "image": "mcr.microsoft.com/devcontainers/node:20",

  "forwardPorts": [3000],

  "postCreateCommand": "npm install",
  "postStartCommand": "npm run dev",

  "portsAttributes": {
